;; -*- mode: emacs-lisp; coding: utf-8-unix -*-
;; Minibuffer history file, automatically generated by ‘savehist’.

(setq savehist-minibuffer-history-variables '(consult--line-history extended-command-history consult--buffer-history))
(setq consult--line-history '("screen"))
(setq extended-command-history '("nv-emacs-configuration-reload" "kill-emacs" "nv-emacs-configuration-reload" "nv-display-configuration-reload" "eb" "nv-display-configuration-reload"))
(setq kill-ring 'nil)
(setq search-ring 'nil)
(setq regexp-search-ring 'nil)
